{
  "name": "fly-keys",
  "displayName": "Fly Keys",
  "description": "effective modal editing",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCommand:type"
  ],
  "contributes": {
    "keybindings": [
      {
        "key": "cmd+space",
        "command": "extension.flyKeys.activeCommandMode",
        "when": "editorTextFocus"
      },
      {
        "key": "f",
        "command": "extension.flyKeys.deactiveCommandMode",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "i",
        "command": "cursorUp",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "k",
        "command": "cursorDown",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "j",
        "command": "cursorLeft",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "l",
        "command": "cursorRight",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "d",
        "command": "deleteRight",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "e",
        "command": "deleteWordLeft",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "h",
        "command": "cursorHome",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "n",
        "command": "scrollPageDown",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "p",
        "command": "scrollPageUp",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "o",
        "command": "cursorWordRight",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "q",
        "command": "workbench.action.files.save",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "r",
        "command": "deleteWordRight",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "s",
        "command": "editor.action.smartSelect.grow",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "t",
        "command": "cancelSelection",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "u",
        "command": "cursorWordLeft",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "y",
        "command": "undo",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "/",
        "command": "editor.action.jumpToBracket",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": ";",
        "command": "cursorEnd",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "z",
        "command": "extension.flyKeys.commentLine",
        "when": "editorTextFocus && extension.flyKeys.commandMode && !editorReadonly"
      },
      {
        "key": "x",
        "command": "extension.flyKeys.cut",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "c",
        "command": "extension.flyKeys.copy",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      },
      {
        "key": "v",
        "command": "extension.flyKeys.paste",
        "when": "editorTextFocus && extension.flyKeys.commandMode"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
